          <type>uint8_t *</type>
          <declname>inpt</declname>
        </param>
        <param>
          <type>uint8_t *</type>
          <declname>outt</declname>
        </param>
        <param>
          <type>uint8_t *</type>
          <declname>ivec</declname>
        </param>
        <param>
